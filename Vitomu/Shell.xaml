<controls:VitomuWindow x:Class="Vitomu.Shell"
                       xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                       xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
                       xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                       xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
                       xmlns:dc="clr-namespace:Digimezzo.WPFControls;assembly=Digimezzo.WPFControls"
                       xmlns:controls="clr-namespace:Vitomu.Controls"
                       mc:Ignorable="d"
                       Background="{DynamicResource Brush_DarkGrey}"
                       Title="Vitomu"
                       Loaded="Shell_Loaded"
                       MinHeight="450"
                       MinWidth="450"
                       Width="450"
                       Height="450"
                       KeyDown="VitomuWindow_KeyDown">

    <Frame x:Name="ShellFrame" NavigationUIVisibility="Hidden"/>
</controls:VitomuWindow>
